@model IEnumerable<Demofolio.Models.GitHubAPI>

<h2>Repos with the Highest Star Count</h2>
<hr />

	@foreach (var repo in Model)
	{
		<div>
				<h4><a href="@Html.DisplayFor(modelItem => repo.html_url)">@Html.DisplayFor(modelItem => repo.name)</a></h4>
				<li>Description: @Html.DisplayFor(modelItem => repo.description)</li>
				<li>Star Count: @Html.DisplayFor(modelItem => repo.stargazers_count)</li>
		</div>
	}
<br /><br />

@Html.ActionLink("Back home", "Index")
